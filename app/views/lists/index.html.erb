<section class="todoapp">
			<header class="header">
				<h1> Your List</h1>
				<%= form_for(List.new) do |f| %>
                    <%= f.text_field :name, :class => "new-todo" %>
                    <%#input class="new-todo" type="text" name="list[name]" id="list_name"%>
                    <%= f.submit :style => "display:none"%>
                <% end %>
                </form>
               
			</header>
			<section class="main">
                    <input class="toggle-all" type="checkbox">
                    <label for="toggle-all"> Mark ask complete </label>
                <ul class="todo-list">
                <% @lists.each do |list| %>
                    <li>
                    <div class="view">
                        <label> <%= link_to list.name, list_path(list) %> </label>
                        </div>
                    </li>
                     <% end %>
			</section>
		
		</section>