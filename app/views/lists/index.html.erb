<section class="todoapp">
			<header class="header">
				<h1>Todo List</h1>
				<input class="new-todo" placeholder="Make a new list" autofocus="">
			</header>
			<section class="main">
                    <input class="toggle-all" type="checkbox">
                    <label for="toggle-all"> Mark ask complete </label>
                <ul class="todo-list">
                <% @lists.each do |list| %>
                    <li>
                    <div class="view">
                        <label> <%= link_to list.name, list_path(list) %> </label>
                        </div>
                    </li>
                     <% end %>
			</section>
		
		</section>